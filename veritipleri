sayilar=[1,2,3,4,5]
isimler=["ali", "mehmet", "ayşe"]
sayi1=10
print(type(sayi1))
print(type(sayilar))
print(type(isimler))
#indeks numaraları sıfırla başlar ali 0 mehmet1 ayşe 2 dir
print(isimler[2])
#yukarıdaki gibi gösterilebilir indeks sayısı.
#eğer sayı listede de sonlardaysa - den başlarız.
print(isimler[-3])
#python önceden aynı tip verileri saklıyordu şimdi 10. sürüm ve üstü farklı tipteki verileri saklayabilir.
ogrenci1=["ali", "yılmaz", "2002029", 60,50,70]
print(ogrenci1[0] + " " + ogrenci1[1])
ortalama=(ogrenci1[3]+ogrenci1[4]+ogrenci1[5])/3
print("ortalamanız:" + str(ortalama))
# ortalama int veri tipi dir fakat ortalamayı str parantezinine alınca str gibi davranır yukarıdaki örnekteki gibi
programlama_dilleri = ["pyhton", "c#", "java"]
#güncelleme yapalım
programlama_dilleri[2] = "C++"
print(programlama_dilleri) 
sonuc = len(programlama_dilleri)
sonuc = programlama_dilleri + ["go","delphi"]
sonuc = "pyhton" in programlama_dilleri
sonuc = "react" in programlama_dilleri
#koşul ifadeleri
del programlama_dilleri[0]
print(programlama_dilleri)
sehirler= ['izmir', 'istanbul']
plakalar= [35,34]
print(plakalar[0], sehirler[0])
print(plakalar[1], sehirler[1])
#dictionary
plakalar= { 'kocaeli':41, 'istanbul': 34, 'izmir': 36}
#yanlışı düzeltebilir
plakalar['izmir'] = 35
print(plakalar['kocaeli'])
print(plakalar['istanbul'])
print(plakalar['izmir'])
